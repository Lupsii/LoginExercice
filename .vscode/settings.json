{
    "vetur.format.defaultFormatter.js": "prettier-eslint",
    "i18n-ally.pathMatcher": "{locale}.json",
    "i18n-ally.enabledParsers": "json",
    "i18n-ally.localesPaths": "frontend/src/locales",
    "i18n-ally.keystyle": "nested",
    "i18n-ally.enabledFrameworks": "vue",
    "i18n-ally.editor.preferEditor": true,
    "i18n-ally.displayLanguage": "en",
    "i18n-ally.sourceLanguage": "en",
    "i18n-ally.keysInUse": [
        "$vuetify.badge",
        "$vuetify.close",
        "$vuetify.dataIterator.noResultsText",
        "$vuetify.dataIterator.loadingText",
        "$vuetify.dataTable.itemsPerPageText",
        "$vuetify.dataTable.ariaLabel.sortDescending",
        "$vuetify.dataTable.ariaLabel.sortAscending",
        "$vuetify.dataTable.ariaLabel.sortNone",
        "$vuetify.dataTable.ariaLabel.activateDescending",
        "$vuetify.dataTable.ariaLabel.activateNone",
        "$vuetify.dataTable.ariaLabel.activateAscending",
        "$vuetify.dataTable.sortBy",
        "$vuetify.dataFooter.itemsPerPageText",
        "$vuetify.dataFooter.itemsPerPageAll",
        "$vuetify.dataFooter.nextPage",
        "$vuetify.dataFooter.prevPage",
        "$vuetify.dataFooter.firstPage",
        "$vuetify.dataFooter.lastPage",
        "$vuetify.dataFooter.pageText",
        "$vuetify.datePicker.itemsSelected",
        "$vuetify.noDataText",
        "$vuetify.carousel.prev",
        "$vuetify.carousel.next",
        "$vuetify.carousel.ariaLabel.delimiter",
        "$vuetify.calendar.moreEvents",
        "$vuetify.fileInput.counter",
        "$vuetify.fileInput.counterSize",
        "$vuetify.timePicker.am",
        "$vuetify.timePicker.pm",
        "errors.validator.required",
        "errors.validator.isEmail",
        "errors.server.unauthorized",
        "errors.server.validation",
        "errors.server.technical",
        "errors.server.unexpected",
        "errors.server.wrongCredentials",
        "errors.validator.isUrl",
        "errors.validator.nonNegative",
        "errors.validator.isLength",
        "errors.validator.minLength",
        "errors.validator.maxLength",
        "errors.server.notreachable",
        "errors.validator.passwordMatch"
    ],
    "search.exclude": {
        "**/dist": true
    },
    "files.exclude": {
        "**/.project": true
    },
    "files.watcherExclude": {
        "**/dist/**": true
    }
}